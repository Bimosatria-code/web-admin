                <option value="0" <%= transfer.request_activation ? '' : 'selected' %>>Non Aktif</option>
              </select>
            </div>
          </div> -->
          <div class="form-group row">
            <label class="col-sm-2 col-form-label">Jenis Transaksi</label>
            <div class="col-sm-3">
              <select class="form-control" id="description_pcode" name="description_pcode" required>
                <% for (let trans of master) { %>
                  <option value="<%= trans.id %>" <%= transfer.description_pcode === trans.id ? 'selected' : '' %>><%= trans.transactions %></option>
                <% } %>
              </select>
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-2 col-form-label">Pcode</label>
            <div class="col-sm-3">
              <input type="text" 
                      class="form-control" 
                      id="pcode" 
                      name="pcode"
                      minlength="6"
                      maxlength="6" 
                      value="<%= transfer.pcode %>"
                      readonly
                      required />
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-2 col-form-label">Jenis Transaksi</label>
            <div class="col-sm-3">
              <input type="text" 
                      class="form-control" 
                      id="jenis_transaksi" 
                      name="jenis_transaksi" 
                      value="<%= transfer.jenis_transaksi %>"
                       />
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-2 col-form-label">Deskripsi Trx</label>
            <div class="col-sm-3">
              <input type="text" 
                      class="form-control" 
                      id="trx_dec" 
                      name="trx_dec" 
                      minlength="1"
                      maxlength="40" 
                      value="<%= transfer.trx_dec %>" />
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-2 col-form-label">Channel ID</label>
