<script>
  // Get pcode, biller code, product type and product code
  $(document).ready(function(){
    $("#description_pcode").change(function(){
      var id = this.value;
      ajax_request(id);
    });
  });

  function check_row(sequence, channel) {
    var fee_ib = $("input[name=am_"+channel+"_"+sequence+"]").val();
    var fee_mb = $("input[name=am_"+channel+"_"+sequence+"]").val();
    var fee_ibb = $("input[name=am_"+channel+"_"+sequence+"]").val();

    if(parseInt(fee_ib) > 0 || parseInt(fee_mb) > 0 || parseInt(fee_ibb) > 0) {
      document.getElementById("ori_"+channel+"_"+sequence).required = true;
      document.getElementById("rev_"+channel+"_"+sequence).required = true;
      // document.getElementById("acc_"+channel+"_"+sequence).required = true;
    } else {
      document.getElementById("ori_"+channel+"_"+sequence).required = false;
      document.getElementById("rev_"+channel+"_"+sequence).required = false;
      // document.getElementById("acc_"+channel+"_"+sequence).required = false;
    }
  }
  
  function ajax_request(id){
    $.ajax({
      url: "/select-master/" + id,
      type:"get",
      success:function(response){
      	if (response.code === 500) {
          Swal.fire(
            response.label,
            response.message,
            response.status
          )
        }

        // set value
        document.getElementById("pcode").setAttribute('value', response.message.pcode);
      },
      error: function(error) {
        console.log(error);
      }
    });
